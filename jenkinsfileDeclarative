pipeline{
agent any
stages{
		stage('checkout'){
		steps{
			echo 'Checking out !!'
			git 'https://github.com/vamsibkrishna/spring-petclinic.git'
			}
		}

	stage('build'){
	environment{
		env.PATH = "C:/Users/pc/Downloads/apache-maven-3.6.3/bin;c:\\Windows\\System32"
	}
	steps{
			echo 'Building out !!'
			bat label: '', script: 'mvn package'
		}
		
	}

	stage('archiving'){
		steps{
			echo 'archiving out !!'
			archiveArtifacts 'target/*.jar'
		}
		

	}
}
}